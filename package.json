    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/runtime": "^7.17.8",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
