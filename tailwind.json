{
	"mb-5": {
		"style": {
			"marginBottom": 20
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"border-white": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(255 255 255 / var(--tw-border-opacity))"
		}
	},
	"px-2": {
		"style": {
			"paddingLeft": 8,
			"paddingRight": 8
		}
	},
	"py-3": {
		"style": {
			"paddingTop": 12,
			"paddingBottom": 12
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-6": {
		"style": {
			"paddingTop": 24,
			"paddingBottom": 24
		}
	},
	"px-1.5": {
		"style": {
			"paddingLeft": 6,
			"paddingRight": 6
		}
	},
	"px-1": {
