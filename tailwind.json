{
	"mb-5": {
		"style": {
			"marginBottom": 20
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-screen": {
		"style": {
			"height": "100vh"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"rounded": {
		"style": {
			"borderTopLeftRadius": 4,
			"borderTopRightRadius": 4,
			"borderBottomRightRadius": 4,
			"borderBottomLeftRadius": 4
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"border-white": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(255 255 255 / var(--tw-border-opacity))"
		}
	},
	"bg-yellow-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(254 240 138 / var(--tw-bg-opacity))"
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"bg-gray-300": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(209 213 219 / var(--tw-bg-opacity))"
		}
	},
	"px-2": {
		"style": {
			"paddingLeft": 8,
			"paddingRight": 8
		}
	},
	"py-3": {
		"style": {
			"paddingTop": 12,
			"paddingBottom": 12
		}
	},
	"px-3": {
		"style": {
			"paddingLeft": 12,
			"paddingRight": 12
		}
	},
	"py-6": {
		"style": {
			"paddingTop": 24,
			"paddingBottom": 24
		}
	},
	"px-1.5": {
		"style": {
			"paddingLeft": 6,
			"paddingRight": 6
		}
	},
	"px-1": {
		"style": {
			"paddingLeft": 4,
			"paddingRight": 4
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"text-sm": {
		"style": {
			"fontSize": 14,
			"lineHeight": 20
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"text-blue-300": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(147 197 253 / var(--tw-text-opacity))"
		}
	},
	"text-gray-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(31 41 55 / var(--tw-text-opacity))"
		}
	},
	"text-gray-600": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(75 85 99 / var(--tw-text-opacity))"
		}
	},
	"dark:border-white": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(255 255 255 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(255 255 255 / var(--tw-border-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	},
	"dark:bg-black": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 0 0 / var(--tw-bg-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	},
	"dark:text-yellow-200": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(254 240 138 / var(--tw-text-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	},
	"dark:text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	}
}